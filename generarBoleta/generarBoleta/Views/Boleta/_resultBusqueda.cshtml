@model IEnumerable<generarBoleta.Models.dataFind>
@{
    ViewBag.Title = "_resultBusqueda";
}
    <div class="row">
        @foreach (var item in Model)
        {

            <div class="col-sm-4">
                <br />
                <div class="card">
                    <div class="card-header">
                        <center><strong>@Html.DisplayFor(modelItem => item.placa)</strong></center>
                    </div>
                    <div class="card-body">
                        <h5 class="card-title"><strong>Propietario: </strong>@Html.DisplayFor(modelItem => item.propietario)</h5>
                        <p class="card-text"><strong>Descripcion</strong>@Html.DisplayFor(modelItem => item.descripcion)</p>
                    </div>
                    <ul class="list-group list-group-flush">
                        <li class="list-group-item"><strong>Tiempo</strong></li>
                        <li class="list-group-item"><strong>Inicio:@Html.DisplayFor(modelItem => item.inicioHora)</strong> </li>
                        <li class="list-group-item"><strong>Final:</strong> @Html.DisplayFor(modelItem => item.finalHora)</li>

                        <li class="list-group-item"><strong>Total:@Html.DisplayFor(modelItem => item.total)</strong></li>

                    </ul>
                    <div class="card-body">
                        <a href="#" class="btn btn-primary">Detalles</a>
                        <a href="#" class="btn btn-success">Pagado</a>
                    </div>
                </div>
            </div>

        }
    </div>
